{
  "name": "weather-app-course",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "engines": {
    "node": ">=20",
    "yarn": ">=1.22.17"
  },
  "scripts": {
    "reset-project": "node ./scripts/reset-project.js",
    "prepare": "husky install",
    "lint": "expo lint",
    "build:api-key:ios": "eas build -p ios --profile preview",
    "build:bundle:ios": "eas build -p ios",
    "build:api-key:android": "eas build -p android --profile preview",
    "build:bundle:android": "eas build -p android",
    "dev": "expo start --port 8080",
    "dev:android": "expo start --android --port 8081",
    "dev:ios": "expo start --ios",
    "dev:web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@shopify/react-native-skia": "1.2.3",
    "expo": "~51.0.28",
    "expo-blur": "^13.0.2",
    "expo-constants": "~16.0.2",
    "expo-font": "^12.0.10",
    "expo-linking": "~6.3.1",
    "expo-location": "^17.0.1",
    "expo-router": "^3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "^3.0.7",
    "expo-web-browser": "^13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.79",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "typescript": "~5.3.3"
  },
  "private": true
}
